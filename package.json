    "postcss": "^8.4.23",
    "proj4": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-kakao-maps-sdk": "^1.1.9",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.1",
    "recoil": "^0.7.7",
    "styled-components": "^6.0.0-rc.3",
    "tailwindcss": "^3.3.2"
